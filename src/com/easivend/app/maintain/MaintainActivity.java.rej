+	//读取文件信息
+	boolean  ReadSharedPreferencesAccess()
+	{
+		boolean isaccess=true;
+		//文件是私有的
+		SharedPreferences  user = getSharedPreferences("access_info",0);
+		//读取
+		isaccess=user.getBoolean("access",true);
+		return isaccess;
+	}
+	//写入文件信息
+	private void  WriteSharedPreferences(boolean value)
+	{
+		//文件是私有的
+		SharedPreferences  user = getSharedPreferences("access_info",0);
+		//需要接口进行编辑
+		SharedPreferences.Editor edit=user.edit();
+		//设置
+		edit.putBoolean("access", value);
+		//提交更新
+		edit.commit();
+	}
